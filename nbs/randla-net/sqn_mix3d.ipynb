    "# Augmentation: Understanding point_per_cut\n",
    "\n",
    "\n",
    "def crop(points, x_min, y_min, z_min, x_max, y_max, z_max):\n",
    "    if x_max <= x_min or y_max <= y_min or z_max <= z_min:\n",
    "        raise ValueError(\n",
    "            \"We should have x_min < x_max and y_min < y_max and z_min < z_max. But we got\"\n",
    "            \" (x_min = {x_min}, y_min = {y_min}, z_min = {z_min},\"\n",
    "            \" x_max = {x_max}, y_max = {y_max}, z_max = {z_max})\".format(\n",
    "                x_min=x_min,\n",
    "                x_max=x_max,\n",
    "                y_min=y_min,\n",
    "                y_max=y_max,\n",
    "                z_min=z_min,\n",
    "                z_max=z_max,\n",
    "            )\n",
    "        )\n",
    "    inds = np.all(\n",
    "        [\n",
    "            (points[:, 0] >= x_min),\n",
    "            (points[:, 0] < x_max),\n",
    "            (points[:, 1] >= y_min),\n",
    "            (points[:, 1] < y_max),\n",
    "            (points[:, 2] >= z_min),\n",
    "            (points[:, 2] < z_max),\n",
    "        ],\n",
    "        axis=0,\n",
    "    )\n",
    "    return inds\n",
    "\n",
    "\n"
   ]
  },
